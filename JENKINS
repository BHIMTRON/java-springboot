pipeline {
    agent any
    stages {
        stage('Buils') {
            steps {
                echo 'Hello"
            }
        }
        stage('Test') {
            steps {
                echo 'Hello"
            }
        }
        stage('Sonarqube') {
            steps {
                echo 'Hello"
            }
        }
        stage('Push to Artifactory') {
            steps {
                echo 'Hello"
            }
        }
        
        stage("Deploy to Push') {
            steps {
                echo 'Hello"
            }
        }
    }
    post { 
        failure { 
            echo "failure"
        }
        success{
            echo "success"
        }
        always {
            echo "always hello"
        }
       }

}